<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="shetj.me.base.func.main.MainActivity">

    <com.google.android.material.chip.ChipGroup
        android:id="@+id/group"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent">


        <com.google.android.material.chip.Chip
            android:id="@+id/btn_select_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/string_pic" />

        <com.google.android.material.chip.Chip
            android:id="@+id/btn_setting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/test_setting" />


        <com.google.android.material.chip.Chip
            android:id="@+id/test_loading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="Loading" />

        <com.google.android.material.chip.Chip
            android:id="@+id/test_event"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="@string/test_cla" />

        <com.google.android.material.chip.Chip
            android:id="@+id/btn_test_keybord"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/test_key" />


        <com.google.android.material.chip.Chip
            android:id="@+id/btn_doc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/test_dir"
            android:textAllCaps="false" />

        <com.google.android.material.chip.Chip
            android:id="@+id/btn_gray_model"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/string_gray_them"
            android:textAllCaps="false" />

        <com.google.android.material.chip.Chip
            android:id="@+id/change_language"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/string_change_lang"
            android:textAllCaps="false" />

        <com.google.android.material.chip.Chip
            android:id="@+id/start_ac2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/string_go_test"
            android:textAllCaps="false" />

        <com.google.android.material.chip.Chip
            android:id="@+id/start_power"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/ig_power"
            android:textAllCaps="false" />

        <com.google.android.material.chip.Chip
            android:id="@+id/showSideDialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="SideSheetDialog" />

        <com.google.android.material.chip.Chip
            android:id="@+id/btn_change_theme"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="MDThemeKit"/>

        <com.google.android.material.chip.Chip
            android:id="@+id/btn_SlidingPane"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="SlidingPane"/>

        <com.google.android.material.chip.Chip
            android:id="@+id/btn_perm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="外部存储权限"/>

        <com.google.android.material.chip.Chip
            android:id="@+id/btn_wx"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="WX"/>

        <com.google.android.material.chip.Chip
            android:id="@+id/KeyStore"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="KeyStore"/>

        <com.google.android.material.chip.Chip
            android:id="@+id/Preference"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Preference"/>
    </com.google.android.material.chip.ChipGroup>




</androidx.constraintlayout.widget.ConstraintLayout>
